{
  "hosting": {
    "public": "dist/oshop/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "targets": [
      {
        "name": "oshop",
        "hosting": {
          "public": ".",
          "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
          ],
          "rewrites": [
            {
              "source": "**",
              "destination": "/index.html"
            }
          ]
        },
        "frameworksBackend": {
          "region": "us-central1"
        }
      }
    ]
  }
}
